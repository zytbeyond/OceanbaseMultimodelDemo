# OceanBase Connection Configuration
# Copy this file to .env and update with your actual connection details

# OceanBase Connection Parameters
OB_HOST=<YOUR_OB_HOST>  # Default: localhost
OB_PORT=<YOUR_OB_PORT>  # Default: 2881
OB_USER=<YOUR_OB_USER>  # Default: root
OB_PASSWORD=<YOUR_OB_PASSWORD>  # Replace with your actual password
OB_DATABASE=<YOUR_OB_DATABASE>  # Default: test

# MCP Configuration (if using MCP)
MCP_SERVER_NAME=oceanbase
MCP_TOOL_NAME=execute_sql

# AWS Bedrock Configuration (for vector embeddings)
AWS_ACCESS_KEY_ID=<YOUR_AWS_ACCESS_KEY>  # Replace with your AWS access key
AWS_SECRET_ACCESS_KEY=<YOUR_AWS_SECRET_KEY>  # Replace with your AWS secret key
AWS_REGION=us-east-1  # Change if using a different region